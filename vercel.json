{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/cron/soft-cap-evaluation",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/eod-report",
      "schedule": "0 0 * * *"
    }
  ]
}
